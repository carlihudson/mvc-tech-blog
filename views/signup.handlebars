<h3>Create An Account</h3>
<form>
  <div class="signup-form">
    <label for="username-signup">Username</label>
    <input type="text" class="form-control" id="username-signup"  placeholder="Enter username">
  </div>
  <div class="form-group">
    <label for="password-signup">Password</label>
    <input type="password" class="form-control" id="password-signup" placeholder="Password">
  </div>
  <button id="signup-button" class="btn btn-primary ">Sign Up!</button>
</form>

<script src="../js/signup.js"></script>